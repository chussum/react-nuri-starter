{
    "presets": [
        "es2015",
        "react"
    ],
    "plugins": [
        "transform-class-properties",
        "transform-object-rest-spread",
        "transform-flow-strip-types",
        [
            "module-resolver",
            {
                "root": [
                    "./src"
                ],
                "alias": {
                    "nuri": "./src/_/external/nuri",
                    "asset-require-hook": "./src/_/external/hooks/assets",
                },
                "cwd": "babelrc"
            }
        ]
    ]
}
