{
  "name": "nuri",
  "version": "0.0.1",
  "description": "Simple framework for isomorphic web sites",
  "main": "index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-register",
    "build": "babel -d ./build src/",
    "start:dev": "webpack --context src/ --config webpack.config.js && babel-node src/server.js",
    "start:prod": "NODE_ENV=production webpack --context src/ --config webpack.config.js && babel-node src/server.js",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "lint": "eslint src/"
  },
  "license": "ISC",
